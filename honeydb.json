                    "description": "IP to lookup",
                    "data_type": "string",
                    "required": true,
                    "primary": true,
                    "contains": [
                        "ip"
                    ],
                    "order": 0
                },
                "feed": {
                    "description": "Source feed: 'Both', 'Bad Hosts', 'Twitter'",
                    "data_type": "string",
                    "value_list": [
                        "Both",
                        "Bad Hosts",
                        "Twitter"
                    ],
                    "default": "Both",
                    "order": 1
                }
            },
